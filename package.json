{
  "private": true,
  "name": "lens-extension-cc",
  "publisher": "mirantis",
  "version": "0.0.1",
  "description": "Lens extension that loads Mirantis Container Cloud clusters.",
  "engines": {
    "lens": ">= 4.0.0"
  },
  "main": "dist/main.js",
  "renderer": "dist/renderer.js",
  "files": [
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && webpack",
    "dev": "TARGET=development npm run build",
    "fmt": "prettier --write \"{*,src/**/*}.+(js|ts*|yml|json)\"",
    "fmt:check": "prettier --check \"{*,src/**/*}.+(js|ts*|yml|json)\"",
    "lint": "eslint \"{*,src/**/*}.+(js|ts*)\"",
    "prepublishOnly": "npm run build",
    "start": "TARGET=development npm run build -- --watch",
    "test": "npm run lint && npm run fmt:check"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@emotion/styled-base": "^10.0.31",
    "@k8slens/extensions": "^4.0.0-alpha.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.35",
    "@types/react-router": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "emotion-theming": "^10.0.27",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rtvjs": "^2.4.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11"
  }
}
